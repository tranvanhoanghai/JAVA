+package com.company.designpattern.creational.abstractfactory;
+
+public class BookStore implements IBookStore {
+    private StoreLocation location;
+
+    public BookStore(StoreLocation location) {
+        this.location = location;
+    }
+
+    @Override
+    public IDistributor getDistributor() {
+        IDistributor iDistributor = null;
+        switch (location) {
+            case Q1:
+                iDistributor = new Q1Distributor();
+                break;
+            case Q2:
+                iDistributor = new Q2Distributor();
+                break;
+            case Q3:
+                iDistributor = new Q3Distributor();
+                break;
+        }
+        return iDistributor;
+    }
+
+    @Override
+    public IAdvertiser getAdvertiser() {
+        return null;
+    }
+}
